{
  "name": "cli-toris",
  "version": "1.0.0",
  "description": "A simple cli to start nodejs project from template",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "start": "ts-node src/index.ts",
    "watch": "nodemon --watch src --exec ts-node src/index.ts",
    "build": "tsc && shx rm -rf dist/templates && shx cp -r src/templates dist"
  },
  "bin": {
    "cli-toris": "./dist/index.js"
  },
  "keywords": [],
  "author": "mattinove",
  "license": "ISC",
  "dependencies": {
    "ejs": "^3.1.6",
    "inquirer": "^8.0.0",
    "shelljs": "^0.8.4",
    "yargs": "^16.2.0"
  },
  "devDependencies": {
    "@types/ejs": "^3.0.6",
    "@types/inquirer": "^7.3.1",
    "@types/node": "^14.14.37",
    "@types/shelljs": "^0.8.8",
    "@types/yargs": "^16.0.1",
    "nodemon": "^2.0.7",
    "shx": "^0.3.3",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3"
  }
}
